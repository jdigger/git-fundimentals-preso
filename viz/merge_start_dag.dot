digraph merge_start_dag {
    bgcolor=transparent;
    fontcolor=black;
    ratio=fill;
    node [shape=box, style="rounded,filled" fontsize="14"];
    edge [fontsize="14"];

    subgraph /* Refs */ {
        node [fillcolor=white];
        HEAD
        BR1
        BR2
    }
    subgraph /* Commits */ {
        node [fillcolor=yellow];
        c1;
        c2;
        c3;
    }
    subgraph /* Trees */ {
        node [fillcolor=chartreuse];
        t1;
        t2;
        t3;
    }
    subgraph /* Blobs */ {
        node [fillcolor=cyan];
        b1;
        b2;
        b3;
        b4;
        b5;
    }
    subgraph /* edges */ {
        edge [fontsize="9"]
        HEAD -> BR1 [constraint=false]
        BR1 -> c2 [constraint=false]
        BR2 -> c3 [constraint=false]
        c1 -> t1
        t1 -> {b1 b2 b3}
        t2 -> {b1 b2 b4}
        t3 -> {b2 b3 b5}
        c2 -> {c1 t2}
        c3 -> {c1 t3}
    }
}
